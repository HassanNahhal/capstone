{
  "name": "Item",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true
    },
    "id": {
      "type": "number",
      "id": true,
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "receiptItem": {
      "type": "belongsTo",
      "model": "ReceiptItem",
      "foreignKey": "itemId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
