{
  "name": "Receipt",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "numberOfItem": {
      "type": "number",
      "required": true
    },
    "total": {
      "type": "number",
      "required": true
    },
    "error": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "comment": {
      "type": "string"
    },
    "imageFilePath": {
      "type": "string",
      "required": true
    },
    "storeId": {
      "type": "number",
      "id": false,
      "required": true
    },
    "id": {
      "type": "number",
      "id": true,
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customerId"
    },
    "receiptItems": {
      "type": "hasMany",
      "model": "ReceiptItem",
      "foreignKey": ""
    },
    "receiptItem": {
      "type": "belongsTo",
      "model": "ReceiptItem",
      "foreignKey": "receiptId"
    },
    "store": {
      "type": "belongsTo",
      "model": "Store",
      "foreignKey": "receiptId"
    },
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "categoryId"
    }
  },
  "acls": [
