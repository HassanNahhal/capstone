{
  "name": "Customer",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "dateOfBirth": {
      "type": "date"
    },
    "gender": {
      "type": "number"
    },
    "accountTypeId": {
      "type": "number",
      "id": false
    }
  },
  "validations": [],
  "relations": {
    "receipts": {
      "type": "hasMany",
      "model": "Receipt",
      "foreignKey": "customerId"
    },
    "accountType": {
      "type": "belongsTo",
      "model": "AccountType",
      "foreignKey": "customerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
